    "@testing-library/react": "^16.0.0",
    "@types/node": "22.19.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/react-is": "19.2.0",
    "jest-environment-jsdom": "29.7.0",
    "react-is": "19.2.0",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "tsup": "^8.2.4",
    "typescript": "5.9.3",
